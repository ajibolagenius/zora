{
  "name": "zora",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "dev:mobile": "turbo dev --filter=@zora/mobile",
    "dev:web": "turbo dev --filter=@zora/web",
    "dev:vendor": "turbo dev --filter=@zora/vendor",
    "dev:admin": "turbo dev --filter=@zora/admin",
    "build": "turbo build",
    "build:web": "turbo build --filter=@zora/web",
    "build:vendor": "turbo build --filter=@zora/vendor",
    "build:admin": "turbo build --filter=@zora/admin",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "dotenv-cli": "^8.0.0",
    "prettier": "^3.2.5",
    "turbo": "^2.3.0",
    "typescript": "~5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.15.0",
  "dependencies": {
    "@phosphor-icons/react": "^2.1.10"
  }
}
